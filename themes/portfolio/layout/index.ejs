<main class="page-main page-main--index">
  <%- partial('_partials/filter', {active: 'index'}) %>
  <% var posts = get_posts({
    query: {public: 1, featured: 1},
    orderby: 'date',
    order: -1,
    count: 0
  }); %>
  <% posts.each(function(post){ %>
    <%- partial('_partials/projectCard', {project: post}) %>
  <% }) %>
</main>
