<!-- <main class="page-main page-main--index container--wide container--padded"> -->
<main class="page-main page-main--index">
<% var posts = get_posts({
  orderby: 'date',
  order: -1,
  count: 0,
  query: {featured: 1}
}); %>
<% posts.each(function(post){ %>
  <%- partial('_partials/projectPreview', {project: post}) %>
<% }) %>
</main>
<%- partial('_partials/page-footer', {isIndex: true}, {cache: !config.relative_link}) %>
